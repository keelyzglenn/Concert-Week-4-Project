@{
    Layout="Shared/_layout.cshtml";
}
<h2>Venues:</h2>
<hr />
@if (Model.Count != 0)
{
  <ul>
    @foreach (var venue in Model)
    {
      <li><a href="/venues/@venue.GetId()">@venue.GetName()</a></li>
    }
  </ul>
}
<h4><a href="/venue/new">Add a new venue</a></h4>

<h4><a href="/">Return home</a></h4>
