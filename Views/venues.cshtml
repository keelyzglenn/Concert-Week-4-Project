@{
    Layout="Shared/_layout.cshtml";
}

<h2>Venues</h2>

@if (Model.Count != 0)
{
  <ul>
    @foreach (var venue in Model)
    {
      <h4 class="venues"><a href="/venues/@venue.GetId()">@venue.GetName()</a></h4>
    }
  </ul>
}
<hr />
<h4> Dont see the venue your looking for? </h4>
<h4><a href="/venue/new">Add a new one</a></h4>
